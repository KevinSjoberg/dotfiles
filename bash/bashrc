# Terminal device interface options
#
stty -ixon

# Exports
#
export PS1="\W \$ "
export PATH="$PATH:$HOME/bin"
export EDITOR='vim'

# Language version management
#
eval "$(rbenv init -)"
eval "$(pyenv init -)"

# Sources
#
. $(brew --prefix)/etc/bash_completion
. $(brew --prefix)/Cellar/m/0.1.1/share/m/m
