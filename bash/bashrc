# Terminal device interface options
#
stty -ixon

# Exports
#
export PS1="\W\[\033[0;32m\]\$(__git_ps1 ' (%s)')\[\033[0m\] \$ "
export PATH="$PATH:$HOME/bin"
export EDITOR='vim'
export CLICOLOR=1

# Language version management
#
eval "$(rbenv init -)"
eval "$(pyenv init -)"

# Sources
#
. $(brew --prefix)/share/m/m
. $(brew --prefix)/etc/bash_completion
